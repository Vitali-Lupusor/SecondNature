{
  "name": "sn-cron",
  "version": "1.0.0",
  "description": "Second Nature is a lifestyle improvement programme. This is the node cron server.",
  "repository": "https://github.com/BeSecondNature/sn-cron",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "lint": "eslint src/ __tests__ --ext .js,.ts,.tsx",
    "depcheck": "depcheck-ci",
    "prettier": "prettier --list-different --write 'src/**/*.{js,ts,tsx}' '__tests__'",
    "test": "jest --runInBand",
    "test:paymentJobs": "jest --coverage --collectCoverageFrom='jobs/chargeSubscriptionsJob.js' __tests__/jobs/chargeSubscriptionsJob.test.js",
    "typescript": "tsc --noEmit",
    "build": "tsc",
    "start": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "danger": "danger ci",
    "danger:local": "danger pr",
    "emailsConvertToPug": "node ./scripts/emailConvertJsToPug.js",
    "emailsCopyFromWeb": "node ./scripts/emailCopyPugFromWeb.js"
  },
  "author": "Chris Edson",
  "dependencies": {
    "basic-ftp": "^3.8.7",
    "bcrypt-nodejs": "~0.0.3",
    "braintree": "~2.3.0",
    "chance": "^1.1.7",
    "cheerio": "^1.0.0-rc.3",
    "cron": "~1.0.9",
    "csv-stream": "~0.2.0",
    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "github-api": "^3.3.0",
    "google-auth-library": "^5.10.1",
    "googleapis": "^47.0.0",
    "html-to-text": "^5.1.1",
    "mixpanel": "~0.6.0",
    "moment": "~2.22.2",
    "moment-timezone": "^0.5.31",
    "mongodb": "^3.6.2",
    "mongoose": "^5.4.23",
    "nodemailer": "~1.3.0",
    "nodemailer-sendgrid-transport": "~0.2.0",
    "poisson-process": "^1.0.0",
    "prom-client": "^12.0.0",
    "promise-ftp": "^1.3.5",
    "pug": "^3.0.0",
    "redis": "^2.8.0",
    "request": "~2.87.0",
    "socket.io-emitter": "^3.1.1",
    "stripe": "^7.15.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "concurrently": "^5.3.0",
    "danger": "^10.5.0",
    "depcheck-ci": "^1.0.1",
    "eslint": "^7.10.0",
    "jest": "^26.5.2",
    "jest-util": "^26.5.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x"
  },
  "depcheck": {
    "ignore": [
      "jest-util",
      "prettier",
      "pug",
      "ts-jest",
      "@shelf/jest-mongodb",
      "@types/jest",
      "@types/mongoose",
      "@types/node",
      "@typescript-eslint/eslint-plugin"
    ],
    "ignoreDirs": [
      "node_modules"
    ],
    "strict": false,
    "unused": true
  }
}
